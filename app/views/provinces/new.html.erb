<div id="mashera-form">
	
	<% form_for(@province) do |f| %>
	<fieldset class="inlineLabels">
	  <%= f.error_messages %>
	
		<%= render :partial => 'fields', :locals => {:f => f} %>

	  <div class="buttonHolder">
	    <%= f.submit 'Add' %>
	  </div>
	</fieldset>
	<% end %>

	<div id="page-links">
	<%= link_to 'Back', provinces_path %>
	</div>
	
</div>